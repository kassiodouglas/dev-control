    "migrate:rollback": "npx knex --knexfile knexfile.js migrate:rollback",
    "generate:repository": "node scripts/generate-repository.js",
    "generate:handler": "node scripts/generate-handler.js",
    "generate:migration": "node scripts/generate-migration.js"
  },
  "devDependencies": {
    "electron": "^31.7.7"
  },
  "dependencies": {
    "electron-reloader": "^1.2.3",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7"
  }
}
